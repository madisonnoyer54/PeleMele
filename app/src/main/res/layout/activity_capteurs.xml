<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".CapteursActivity">

    <Switch
            android:layout_width="wrap_content"
            android:onClick="switch2"
            android:layout_height="wrap_content" android:id="@+id/switch1" tools:layout_editor_absoluteY="18dp"
            tools:layout_editor_absoluteX="145dp" tools:ignore="MissingConstraints,OnClick"/>

    <com.example.pelemele2.outils.VueCapteurActivity
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/view" tools:layout_editor_absoluteY="3dp"
            tools:ignore="MissingConstraints"/>
</androidx.constraintlayout.widget.ConstraintLayout>